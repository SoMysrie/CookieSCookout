package cookiescookout;

import cookiescookout.ws.WsServer;
import java.sql.Connection;
import java.sql.DriverManager;
import java.sql.SQLException;
import java.sql.Statement;

/**
 * @author SoMysrie
 * 
 * Main application class
 * 
 * Final class because it must not be inherited
 */
public final class App
{
	/**
	 * Main application entry
	 */
	public static void main( final String[] args ) throws ClassNotFoundException, SQLException
	{
		System.out.println("CookieSCookout app...") ;
		
		WsServer ws = new WsServer() ;
		
		ws.start();

		Class.forName("org.postgresql.Driver");
        Connection conn = DriverManager.getConnection("jdbc:postgresql://<adresse du serveur>:<port du serveur>/test", "<nom d'utilisateur>", "<mot de passe>");
        Statement st = conn.createStatement();
        st.execute("INSERT into test VALUES('test tuple')");
        st.close();
	} ;
} ;