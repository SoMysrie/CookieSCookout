package application;

import javafx.beans.property.SimpleStringProperty;

public class Recipe 
{
	public SimpleStringProperty img;
	public SimpleStringProperty title;
	public SimpleStringProperty recipe;
	public SimpleStringProperty ing;
	public SimpleStringProperty score;
	public SimpleStringProperty poll;
	public SimpleStringProperty url;
 
    /*
    private Recipe(String img, String title, String recipe, String ing, String score, String poll, String url) 
    {
        this.img = new SimpleStringProperty(img);
        this.title = new SimpleStringProperty(title);
        this.recipe = new SimpleStringProperty(recipe);
        this.ing = new SimpleStringProperty(ing);
        this.score = new SimpleStringProperty(score);
        this.poll = new SimpleStringProperty(poll);
        this.url = new SimpleStringProperty(url);
    }
    */
 
    public void setImg( SimpleStringProperty img ) 
    {
        this.img = img;
    }
    
    public SimpleStringProperty getImg() 
    {
        return img;
    }
        
    public void setTitle(SimpleStringProperty title) 
    {
        this.title = title;
    }
    
    public String getTitle() 
    {
        return title.get();
    }
    
    public void setRecipe(SimpleStringProperty recipe) 
    {
        this.recipe = recipe;
    }
    
    public String getRecipe() 
    {
        return recipe.get();
    }
    
    public void setIng(SimpleStringProperty ing) 
    {
        this.ing = ing;
    }
    
    public String getIng() 
    {
        return ing.get();
    }
    
    public void setScore(SimpleStringProperty score) 
    {
        this.score = score;
    }
    
    public String getScore() 
    {
        return score.get();
    }
        
    public void setPoll(SimpleStringProperty poll) 
    {
        this.poll = poll;
    }
    
    public String getPoll() 
    {
        return poll.get();
    }
    
    public void setUrl(SimpleStringProperty url) 
    {
        this.url = url;
    }
    
    public String getUrl() 
    {
        return url.get();
    }
    
}
