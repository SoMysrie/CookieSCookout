package application;

import javafx.beans.property.SimpleStringProperty;

public class Recipe 
{
	private final SimpleStringProperty img;
    private final SimpleStringProperty title;
    private final SimpleStringProperty recipe;
    private final SimpleStringProperty ing;
    private final SimpleStringProperty score;
    private final SimpleStringProperty poll;
    private final SimpleStringProperty url;
 
    private Recipe(String img, String title, String recipe, ing, score, poll, url) 
    {
        this.img = new SimpleStringProperty(img);
        this.title = new SimpleStringProperty(title);
        this.recipe = new SimpleStringProperty(recipe);
        this.ing = new SimpleStringProperty(ing);
        this.score = new SimpleStringProperty(score);
        this.poll = new SimpleStringProperty(poll);
        this.url = new SimpleStringProperty(url);
    }
 
    public String getimg() {
        return img.get();
    }
    public void setimg(String img) {
        this.img = img;
    }
        
    public String gettitlte() {
        return title.get();
    }
    public void settitlte(String title) {
        this.title = title;
    }
    
    public String getrecipe() {
        return recipe.get();
    }
    public void setrecipe(String recipe) {
        this.recipe = recipe;
    }
}
